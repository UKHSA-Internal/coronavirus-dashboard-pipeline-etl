{
  "scriptFile": "__init__.py",
  "entryPoint": "dispatcher",
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "v1/dispatch/{uri:?}"
    },
    {
      "name": "latestAvailable",
      "type": "blob",
      "direction": "in",
      "path": "pipeline/info/latest_available",
      "connection": "DeploymentBlobStorage"
    },
    {
      "name": "latestPublished",
      "type": "blob",
      "direction": "in",
      "path": "pipeline/info/latest_published",
      "connection": "DeploymentBlobStorage"
    },
    {
      "name": "latestReleased",
      "type": "blob",
      "direction": "in",
      "path": "publicdata/assets/dispatch/website_timestamp",
      "connection": "DeploymentBlobStorage"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}