{
    "scriptFile": "__init__.py",
    "bindings": [{
            "name": "newData",
            "type": "blobTrigger",
            "direction": "in",
            "path": "publicdata/{blobname}_{blobdate}.json",
            "connection": "DeploymentBlobStorage"
        },
        {
            "name": "casesCsvOut",
            "type": "blob",
            "direction": "out",
            "path": "downloads/csv/dated/coronavirus-cases_{blobdate}.csv",
            "connection": "DeploymentBlobStorage"
        },
        {
            "name": "casesJsonOut",
            "type": "blob",
            "direction": "out",
            "path": "downloads/json/dated/coronavirus-cases_{blobdate}.json",
            "connection": "DeploymentBlobStorage"
        },
        {
            "name": "casesCsvOutLatest",
            "type": "blob",
            "direction": "out",
            "path": "downloads/csv/coronavirus-cases_latest.csv",
            "connection": "DeploymentBlobStorage"
        },
        {
            "name": "casesJsonOutLatest",
            "type": "blob",
            "direction": "out",
            "path": "downloads/json/coronavirus-cases_latest.json",
            "connection": "DeploymentBlobStorage"
        },
        {
            "name": "deathsCsvOut",
            "type": "blob",
            "direction": "out",
            "path": "downloads/csv/dated/coronavirus-deaths_{blobdate}.csv",
            "connection": "DeploymentBlobStorage"
        },
        {
            "name": "deathsJsonOut",
